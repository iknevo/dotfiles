{
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[markdown]": {
    "editor.defaultFormatter": "DavidAnson.vscode-markdownlint"
  },
  "autoimport.doubleQuotes": true,
  "css.lint.unknownAtRules": "ignore",
  "css.lint.unknownProperties": "ignore",
  "diffEditor.ignoreTrimWhitespace": false,
  "editor.accessibilitySupport": "off",
  "editor.bracketPairColorization.enabled": true,
  "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
  "editor.codeActionsOnSave": {
    "source.fixAll": "always",
    "source.sortImports": "always"
  },
  "editor.copyWithSyntaxHighlighting": true,
  "editor.cursorBlinking": "smooth",
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.cursorStyle": "line",
  "editor.cursorWidth": 3,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.detectIndentation": false,
  "editor.emptySelectionClipboard": true,
  // "editor.fontFamily": "JetBrainsMono Nerd Font",
  // "editor.fontFamily": "mononoki",
  "editor.fontFamily": "FiraCode Nerd Font",
  "editor.fontSize": 15,
  "editor.lineHeight": 2.5,
  "editor.fontLigatures": true,
  "editor.formatOnSave": true,
  "editor.glyphMargin": false,
  "editor.guides.bracketPairs": true,
  "editor.guides.bracketPairsHorizontal": true,
  "editor.guides.highlightActiveIndentation": true,
  "editor.guides.indentation": true,
  "editor.lightbulb.enabled": "off",
  "editor.lineNumbers": "relative",
  "editor.linkedEditing": true,
  "editor.matchBrackets": "always",
  "editor.minimap.showSlider": "always",
  "editor.mouseWheelScrollSensitivity": 2,
  "editor.occurrencesHighlight": "singleFile",
  "editor.quickSuggestions": {
    "comments": "off",
    "strings": "on"
  },
  "editor.renderLineHighlight": "gutter",
  "editor.renderWhitespace": "boundary",
  "editor.scrollbar.horizontal": "hidden",
  "editor.scrollbar.horizontalScrollbarSize": 0,
  "editor.scrollbar.vertical": "hidden",
  "editor.scrollbar.verticalScrollbarSize": 0,
  "editor.scrollBeyondLastLine": false,
  "editor.selectionHighlight": true,
  "editor.semanticHighlighting.enabled": true,
  "editor.showFoldingControls": "never",
  "editor.smoothScrolling": true,
  "editor.snippetSuggestions": "inline",
  "editor.stickyScroll.enabled": false,
  "editor.stickyScroll.scrollWithEditor": false,
  "editor.suggest.insertMode": "replace",
  "editor.suggest.preview": false,
  "editor.suggestLineHeight": 36,
  "editor.tabCompletion": "on",
  "editor.tabSize": 2,
  "editor.unicodeHighlight.ambiguousCharacters": false,
  "editor.unicodeHighlight.includeComments": false,
  "editor.unicodeHighlight.includeStrings": false,
  "editor.unicodeHighlight.nonBasicASCII": false,
  "editor.wordSeparators": "`~!@#$%^&*()-=+[{]}\\|;:'\",.<>/?",
  "emmet.includeLanguages": {
    "javascript": "javascriptreact",
    "typescript": "typescriptreact"
  },
  "eslint.enable": true,
  "eslint.run": "onSave",
  "explorer.compactFolders": false,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.confirmPasteNative": false,
  "explorer.decorations.colors": true,
  "explorer.openEditors.visible": 1,
  "files.associations": {
    "*.css": "css"
  },
  "files.autoSave": "onFocusChange",
  "files.trimTrailingWhitespace": true,
  "git.openRepositoryInParentFolders": "always",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "json.schemaDownload.enable": false,
  "less.lint.unknownAtRules": "ignore",
  "less.lint.unknownProperties": "ignore",
  "liveServer.settings.donotShowInfoMsg": true,
  "material-icon-theme.hidesExplorerArrows": true,
  "reactSnippets.settings.importReactOnTop": false,
  "scm.diffDecorations": "all",
  "scm.diffDecorationsGutterPattern": {
    "added": true,
    "modified": true
  },
  "scm.diffDecorationsGutterVisibility": "always",
  "scm.diffDecorationsGutterWidth": 3,
  "scss.lint.unknownAtRules": "ignore",
  "scss.lint.unknownProperties": "ignore",
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.fontFamily": "FiraCode Nerd Font",
  "terminal.integrated.fontSize": 14,
  "terminal.integrated.gpuAcceleration": "on",
  "terminal.integrated.scrollback": 100,
  "terminal.integrated.smoothScrolling": true,
  "vim.easymotion": true,
  "vim.foldfix": true,
  "vim.highlightedyank.color": "#ffffff50",
  "vim.highlightedyank.duration": 150,
  "vim.highlightedyank.enable": true,
  "vim.highlightedyank.textColor": "#000000",
  "vim.hlsearch": false,
  "vim.leader": "<Space>",
  "vim.useSystemClipboard": true,
  "vscode_custom_css.imports": [
    "file:///E:/visual-studio/custom/custom-vscode.css"
  ],
  "window.title": "${rootName}",
  "chat.agent.enabled": false,
  "window.zoomLevel": 0.8,
  "workbench.editor.customLabels.patterns": {
    "**/{layout,page}.jsx": "${dirname} ${filename}",
    "**/{layout,page}.tsx": "${dirname} ${filename}"
  },
  "workbench.editor.editorActionsLocation": "hidden",
  "workbench.editor.empty.hint": "hidden",
  "workbench.editor.enablePreview": false,
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.editor.labelFormat": "short",
  "workbench.editor.tabActionCloseVisibility": true,
  "workbench.iconTheme": "material-icon-theme",
  "workbench.layoutControl.type": "both",
  "workbench.list.smoothScrolling": true,
  "workbench.startupEditor": "none",
  "workbench.tips.enabled": false,
  "workbench.tree.enableStickyScroll": false,
  "workbench.tree.renderIndentGuides": "none",
  "window.customTitleBarVisibility": "auto",
  "editor.gotoLocation.multipleReferences": "goto",
  "editor.gotoLocation.multipleDefinitions": "goto",
  "editor.gotoLocation.multipleDeclarations": "goto",
  "editor.gotoLocation.multipleImplementations": "goto",
  "editor.gotoLocation.multipleTypeDefinitions": "goto",
  "git.confirmSync": false,
  "workbench.productIconTheme": "fluent-icons",
  "editor.wordWrap": "on",
  "editor.wordWrapColumn": 80,
  "editor.wrappingIndent": "same",
  "workbench.activityBar.location": "bottom",
  "workbench.editor.showTabs": "multiple",
  "workbench.statusBar.visible": true,
  "files.insertFinalNewline": true,
  "editor.overviewRulerBorder": false,
  "editor.hideCursorInOverviewRuler": true,
  "extensions.ignoreRecommendations": true,
  "editor.hover.enabled": true,
  "svelte.enable-ts-plugin": true,
  "vscord.status.idle.check": false,
  "vscord.status.idle.enabled": false,
  "vscord.status.problems.enabled": false,
  "vscord.status.details.text.idle": "Probably making coffee!",
  "vscord.status.details.text.notInFile": "Probably making coffee!",
  "vscord.status.details.text.viewing": "{workspace}",
  "vscord.status.state.text.editing": "Editing {file_name}{file_extension}",
  "vscord.status.details.text.editing": "{workspace}",
  "vscord.status.state.text.viewing": "Editing {file_name}{file_extension}",
  "editor.unicodeHighlight.invisibleCharacters": false,
  "vscord.app.privacyMode.enable": false,
  "vscord.status.details.idle.enabled": false,
  "vscord.behaviour.suppressNotifications": true,
  "chat.commandCenter.enabled": false,
  "workbench.layoutControl.enabled": false,
  "window.commandCenter": false,
  "window.menuBarVisibility": "compact",
  "workbench.colorTheme": "NEVO",
  "currentFilePath.defaultPathStartsFrom": "workSpace",
  "currentFilePath.defaultPathStyle": "windows",
  "breadcrumbs.enabled": false,
  "editor.minimap.enabled": false
}
