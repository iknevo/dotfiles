{
  "liveServer.settings.donotShowInfoMsg": true,
  "editor.fontSize": 20,
  "editor.cursorSmoothCaretAnimation": "on",
  "terminal.integrated.smoothScrolling": true,
  "editor.formatOnSave": true,
  "editor.linkedEditing": true,
  "editor.tabSize": 2,
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.minimap.showSlider": "always",
  "git.autofetch": true,
  "git.openRepositoryInParentFolders": "always",
  "terminal.integrated.cursorBlinking": true,
  "editor.guides.bracketPairsHorizontal": true,
  "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
  "editor.guides.bracketPairs": true,
  "editor.wordWrap": "on",
  "editor.mouseWheelScrollSensitivity": 2,
  "editor.scrollbar.verticalScrollbarSize": 0,
  "workbench.list.smoothScrolling": true,
  "editor.smoothScrolling": true,
  "workbench.iconTheme": "material-icon-theme",
  "editor.stickyScroll.enabled": false,
  "editor.renderWhitespace": "all",
  "workbench.editor.enablePreview": false,
  "vim.easymotion": true,
  "vim.hlsearch": false,
  "vim.foldfix": true,
  "vim.useSystemClipboard": true,
  "vim.highlightedyank.enable": true,
  "vim.highlightedyank.color": "#8ce99a",
  "vim.highlightedyank.textColor": "black",
  "vim.leader": "<Space>",
  "editor.lineNumbers": "relative",
  "[markdown]": {
    "editor.defaultFormatter": "DavidAnson.vscode-markdownlint"
  },
  "editor.suggest.insertMode": "replace",
  "path-intellisense.absolutePathToWorkspace": false,
  "workbench.editor.customLabels.patterns": {
    "**/{layout,page}.tsx": "${dirname} ${filename}",
    "**/{layout,page}.jsx": "${dirname} ${filename}"
  },
  "editor.wordSeparators": "`~!@#%^&*()=+-[{]}\\|;:'\",.<>/?",
  "terminal.integrated.cursorStyle": "line",
  "editor.accessibilitySupport": "off",
  "files.autoSave": "onFocusChange",
  "eslint.run": "onSave",
  "scm.diffDecorationsGutterWidth": 1,
  "scm.diffDecorationsGutterVisibility": "always",
  "hediet.vscode-drawio.resizeImages": null,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "terminal.integrated.fontFamily": "\"JetBrainsMonoNL Nerd Font\"",
  "terminal.integrated.fontSize": 16,
  "editor.fontFamily": "\"JetBrainsMono Nerd Font\",Consolas, 'Courier New', monospace",
  "emmet.includeLanguages": {
    "javascript": "javascriptreact",
    "typescript": "typescriptreact"
  },
  "editor.hideCursorInOverviewRuler": true,
  "window.title": " ",
  "workbench.navigationControl.enabled": false,
  "css.lint.unknownAtRules": "ignore",
  "css.lint.unknownProperties": "ignore",
  "less.lint.unknownAtRules": "ignore",
  "less.lint.unknownProperties": "ignore",
  "scss.lint.unknownAtRules": "ignore",
  "scss.lint.unknownProperties": "ignore",
  "eslint.rules.customizations": [
    {
      "rule": "*",
      "severity": "warn"
    }
  ],
  "files.associations": {
    "*.css": "tailwindcss"
  },
  "editor.copyWithSyntaxHighlighting": false,
  "diffEditor.ignoreTrimWhitespace": false,
  "editor.emptySelectionClipboard": false,
  "files.trimTrailingWhitespace": true,
  "diffEditor.renderSideBySide": false,
  "editor.snippetSuggestions": "top",
  "editor.detectIndentation": false,
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "editor.guides.indentation": false,
  "editor.hover.delay": 500,
  "editor.hover.enabled": true,
  "editor.suggestLineHeight": 30,
  "editor.padding.top": 16,
  "material-icon-theme.files.color": "#42a5f5",
  "workbench.editor.labelFormat": "short",
  "editor.semanticHighlighting.enabled": false,
  "editor.scrollbar.horizontal": "hidden",
  "editor.scrollbar.vertical": "hidden",
  "editor.unicodeHighlight.includeComments": false,
  "editor.unicodeHighlight.includeStrings": false,
  "editor.unicodeHighlight.ambiguousCharacters": false,
  "editor.unicodeHighlight.nonBasicASCII": false,
  "editor.stickyScroll.scrollWithEditor": false,
  "terminal.integrated.env.windows": {},
  "editor.multiCursorLimit": 1000,
  "editor.cursorBlinking": "expand",
  "autoimport.doubleQuotes": true,
  "workbench.layoutControl.enabled": false,
  "window.commandCenter": false,
  "editor.quickSuggestions": {
    "comments": "on",
    "strings": "on"
  },
  "reactSnippets.settings.importReactOnTop": false,
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.productIconTheme": "fluent-icons",
  "workbench.colorTheme": "NEVO",
  "vscord.status.problems.enabled": false,
  "vscord.status.state.text.editing": "Editing  {file_name}{file_extension}",
  "vscord.status.idle.timeout": 500,
  "vscord.status.details.text.viewing": "In {workspace}",
  "vscord.status.buttons.button1.enabled": false,
  "vscord.status.buttons.button1.git.active.enabled": false,
  "vscord.status.buttons.button1.git.idle.enabled": false,
  "vscord.status.buttons.button1.git.inactive.enabled": false,
  "workbench.statusBar.visible": false,
  "workbench.startupEditor": "none",
  "workbench.tips.enabled": false,
  "breadcrumbs.enabled": false,
  "workbench.sideBar.location": "right",
  "editor.minimap.enabled": false,
  "material-icon-theme.hidesExplorerArrows": true,
  "workbench.tree.enableStickyScroll": false,
  "workbench.tree.renderIndentGuides": "none",
  "workbench.tree.indent": 15,
  "explorer.compactFolders": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.confirmDelete": false,
  "window.zoomLevel": 0.25,
  "editor.lineHeight": 2.5,
  "editor.fontLigatures": true,
  "editor.tabCompletion": "on",
  "workbench.activityBar.location": "top",
  "vscode_custom_css.imports": [
    "file:///E:/visual-studio/custom/custom-vscode.css"
  ],
  "workbench.editor.editorActionsLocation": "titleBar"
}
