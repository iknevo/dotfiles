{
  "editor.codeActionsOnSave": {
    "source.fixAll": "always",
    "source.sortImports": "always"
  },
  "editor.accessibilitySupport": "off",
  "editor.suggest.insertMode": "replace",
  "editor.cursorBlinking": "expand",
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.cursorStyle": "line",
  "editor.cursorWidth": 3,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  // "editor.fontFamily": "JetBrainsMono Nerd Font",
  // "editor.fontFamily": "mononoki",
  // "editor.fontFamily": "Dank Mono",
  "editor.fontFamily": "FiraCode Nerd Font",
  "editor.fontSize": 14,
  "editor.lineHeight": 2.2,
  "editor.fontLigatures": true,
  "editor.glyphMargin": false,
  "editor.guides.bracketPairs": "active",
  "editor.guides.bracketPairsHorizontal": "active",
  "editor.guides.highlightActiveIndentation": true,
  "editor.guides.indentation": true,
  "editor.lightbulb.enabled": "off",
  "editor.lineNumbers": "relative",
  "editor.matchBrackets": "always",
  "editor.mouseWheelScrollSensitivity": 2,
  "editor.renderLineHighlight": "none",
  "editor.scrollbar.horizontal": "hidden",
  "editor.scrollbar.horizontalScrollbarSize": 0,
  "editor.scrollbar.vertical": "hidden",
  "editor.scrollbar.verticalScrollbarSize": 0,
  "editor.scrollBeyondLastLine": false,
  "editor.showFoldingControls": "never",
  "editor.smoothScrolling": true,
  "editor.stickyScroll.enabled": false,
  "editor.stickyScroll.scrollWithEditor": false,
  "editor.tabCompletion": "off",
  "editor.tabSize": 2,
  "editor.unicodeHighlight.ambiguousCharacters": false,
  "editor.unicodeHighlight.includeComments": false,
  "editor.unicodeHighlight.includeStrings": false,
  "editor.unicodeHighlight.nonBasicASCII": false,
  "editor.wordSeparators": "~!@#$%^&*()-=+[{]}\\|;:'\",.<>/?",
  "eslint.run": "onSave",
  "explorer.compactFolders": false,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.confirmPasteNative": false,
  "explorer.openEditors.visible": 4,
  "files.associations": {
    "*.css": "css",
    "*.svg": "xml"
  },
  "workbench.editorAssociations": {
    "*.svg": "default"
  },
  "git.openRepositoryInParentFolders": "always",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "reactSnippets.settings.importReactOnTop": false,
  "scm.diffDecorationsGutterPattern": {
    "added": true,
    "modified": true
  },
  "scm.diffDecorationsGutterVisibility": "always",
  "scm.diffDecorationsGutterWidth": 1,
  "terminal.integrated.fontFamily": "FiraCode Nerd Font",
  "terminal.integrated.gpuAcceleration": "on",
  "terminal.integrated.smoothScrolling": true,
  "vim.easymotion": true,
  "vim.foldfix": true,
  "vim.highlightedyank.enable": true,
  "vim.highlightedyank.duration": 150,
  "vim.highlightedyank.color": "#ffffff50",
  "vim.highlightedyank.textColor": "#000000",
  "vim.hlsearch": false,
  "vim.leader": "<Space>",
  "vim.useSystemClipboard": false,
  "vscode_custom_css.imports": [
    "file:///E:/visual-studio/vscode/custom/custom-vscode.css"
  ],
  "window.title": "${rootName}${separator}${activeFolderShort}${separator}${activeEditorShort}${separator}${activeEditorState}${separator}${dirty}",
  // "chat.agent.enabled": false,
  "workbench.editor.customLabels.patterns": {
    "**/{layout,page}.jsx": "${dirname} ${filename}.${extname}",
    "**/{layout,page}.tsx": "${dirname} ${filename}.${extname}"
  },
  "workbench.editor.editorActionsLocation": "hidden",
  "workbench.editor.empty.hint": "hidden",
  "workbench.editor.enablePreview": false,
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.editor.labelFormat": "short",
  "workbench.editor.tabActionCloseVisibility": true,
  "workbench.list.smoothScrolling": true,
  "workbench.startupEditor": "none",
  "workbench.tips.enabled": false,
  "workbench.tree.enableStickyScroll": false,
  "workbench.tree.renderIndentGuides": "none",
  "git.confirmSync": false,
  "workbench.editor.showTabs": "none",
  "extensions.ignoreRecommendations": true,
  "editor.unicodeHighlight.invisibleCharacters": false,
  // "chat.commandCenter.enabled": false,
  "workbench.layoutControl.enabled": false,
  "window.commandCenter": false,
  "window.menuBarVisibility": "compact",
  "workbench.colorTheme": "NEVO",
  "breadcrumbs.enabled": false,
  "workbench.iconTheme": "material-icon-theme",
  "material-icon-theme.activeIconPack": "none",
  "material-icon-theme.hidesExplorerArrows": true,
  "editor.formatOnSave": true,
  "editor.mouseWheelZoom": true,
  "window.zoomLevel": 1,
  "files.exclude": {
    "**/node_modules": true
  },
  "search.searchOnType": false,
  "emmet.includeLanguages": {
    "javascript": "javascriptreact"
  },
  "typescript.updateImportsOnFileMove.enabled": "always",
  "cSpell.userWords": ["wezterm"],
  "chat.agent.enabled": false,
  "chat.commandCenter.enabled": false,
  "workbench.sideBar.location": "right",
  "editor.minimap.enabled": false,
  "workbench.activityBar.location": "top",
  "search.quickOpen.includeHistory": false,
  "search.quickOpen.includeSymbols": false
}
